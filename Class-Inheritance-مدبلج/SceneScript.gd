# يتم استخدام الكلمة الأساسية extends للوراثة من فئة
extends Node

var كائن_الحصان = Horse.new('دارث سيديوس', 'تفاحة')
var كائن_الضفدع = Frog.new()

func _ready():
	كائن_الحصان.eat() # 'دارث سيديوس يريد حقًا أن يأكل تلك التفاحة'
	كائن_الحصان.sleep() # دارث سيديوس نائم
	كائن_الضفدع.eat() # أكل الحشرات
	كائن_الضفدع.طعام = 'القطط'
	كائن_الضفدع.eat() # أكل القطط
